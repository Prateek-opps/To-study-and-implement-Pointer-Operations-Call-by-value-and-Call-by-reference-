#include<iostream>
using namespace std;
void fillArraySequential(int arr[],int&start){
for(int i=0;i<5;i++){
arr[i]=start;
start++;}
}
int main(){
int start;
int arr[5]={10,20,30,40,50};
cout<<"Enter starting number:";
cin>>start;
cout<<"Original array:\n";
for(int i=0;i<5;i++){
cout<<arr[i]<<' ';}
cout<<"\nModified array:\n";
fillArraySequential(arr,start);
for(int i=0;i<5;i++){
cout<<arr[i]<<' ';}
return 0;}
/*Output:-
Enter starting number:12
Original array:
10 20 30 40 50 
Modified array:
12 13 14 15 16 
*/
