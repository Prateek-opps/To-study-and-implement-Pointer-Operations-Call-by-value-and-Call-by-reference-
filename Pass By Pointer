#include <iostream>
using namespace std;
void swap(int *x,int *y) {
    int temp;
    temp=*x;
    *x=*y;
    *y=temp;
}
int main() {
    int a=5;
    int b=10;
    cout<<"Before swapping:"<<endl;
    cout<<"Value of a is: "<<a<<endl;
    cout<<"Value of b is: "<<b<<endl;
    swap(&a,&b);
    cout<<"After swapping:"<<endl;
    cout<<"Value of a is: "<<a<<endl;
    cout<<"Value of b is: "<<b<<endl;
    return 0;
}
/*Output:-
Before swapping:
Value of a is: 5
Value of b is: 10
After swapping:
Value of a is: 10
Value of b is: 5
*/
